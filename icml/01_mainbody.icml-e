              <PathPointType Anchor="300.00000 -425.50000" LeftDirection="300.00000 -425.50000" RightDirection="300.00000 -425.50000" />
            </PathPointArray>
          </GeometryPathType>
        </PathGeometry>
      </Properties>
      <Image Self="ue6" ItemTransform="1.00000 0 0 1.00000 -300.00000 -425.50000">
